<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Show Users</title>
    <link rel="stylesheet" href="showuser.css">
</head>
<body>
    <table>
        <tr>
            <th>ID</th>
            <th>UserName</th>
            <th>Email</th>
        </tr>
        <% for(let user of users){ %>
    <tr>
        <td><%=user.id%></td>
        <td><%=user.username%></td>
        <td><%=user.email%></td>
        <td>
            <form method="get" action="/user/<%= user.id %>/edit">
                <button>Edit Email</button>
            </form>
        </td>
    </tr>
    <% } %>
    </table>
</body>
</html>
